                                $("#promotiontbother").append(SubmithtmlPage);

                                $('#btn_yearly').removeAttr('style');
                                $('#btn_quater').removeAttr('style');
                                $('#btn_other').css("background", "#910000");
                            }
                        }
                    }
                });
            }
        }
