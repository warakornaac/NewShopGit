                                $("#promotiontbquater").append(SubmithtmlPage);
                                $('#btn_yearly').removeAttr('style');
                                $('#btn_quater').css("background", "#910000");
                                $('#btn_other').removeAttr('style');
                            } else if (period == 'other') {
                                $("#promotiontbother").append(SubmithtmlPage);

