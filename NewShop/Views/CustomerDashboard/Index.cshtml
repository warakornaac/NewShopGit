        /* Dropdown Button */

        .dropbtn {
            /* background-color: #04AA6D; */
            color: #075896;
            padding: 7px;
            font-size: 16px;
            border: none;
        }
        /* The container <div> - needed to position the dropdown content */
        .dropdown {
            position: relative;
            display: inline-block;
        }

        .dropdown-content {
            display: none;
            position: absolute;
            background-color: #b1bfce;
            min-width: 160px;
            box-shadow: 0px 8px 16px 0px rgb(0 0 0 / 20%);
            z-index: 999;
