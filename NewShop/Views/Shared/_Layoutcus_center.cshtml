            <script defer>
                //onkeypress="return (event.charCode >= 65 && event.charCode <= 90) || (event.charCode >= 97 && event.charCode <= 122) || (event.charCode >= 48 && event.charCode <= 57)"
                //ถ้ายังไม่กรอกรหัส border input red
                function checkEmptyField(arg) {
                    var id = arg.getAttribute('id');
                    var value = arg.value;
                    if (value.length == 0) {
                        $("#" + id).css({ "border": "1px solid #ff4d4d" });
                        $("#" + id + "Label").css({ "color": "1px solid #ff4d4d" });

                    } else {
                        $("#" + id).css({ "border": "1px solid #5cb85c" });
                        $("#" + id + "Label").css({ "color": "1px solid #5cb85c" });
                    }
                }

                function showOldPassword() {
                    var x = document.getElementById("oldPassword");
                    if (x.type === "password") {
                        x.type = "text";
                    } else {
                        x.type = "password";
                    }
                }

                function showNewPassword() {
                    var x = document.getElementById("newPassword");
                    if (x.type === "password") {
                        x.type = "text";
                    } else {
                        x.type = "password";
                    }
                }

                function showNewConfirmPassword() {
                    var x = document.getElementById("newConfirmPassword");
                    if (x.type === "password") {
                        x.type = "text";
                    } else {
                        x.type = "password";
                    }
                }

                $("#formChangePassword").click(function () {
                    $('#modalChangePassword').modal({ backdrop: 'static', keyboard: false });
                    $('#modalChangePassword').on('shown.bs.modal', function () {

                        $("#oldPassword, #newPassword, #newConfirmPassword").css({ "border": "1px solid #ff4d4d" });
                        $("#oldPasswordLabel, #newPasswordLabel, #newConfirmPasswordLabel").css({ "color": "#ff4d4d" });


                        $("#oldPassword").val('');
                        $("#newPassword").val('');
                        $("#newConfirmPassword").val('');
                        $('#oldPassword').focus()

                    })
                })
                //close modal from
                $('#modalChangePassword').on('hidden.bs.modal', function (e) {
                    //clear warning, success
                    $("#successSave").css('display', 'none');
                    $("#textSuccessSave").html('');
                    $("#warningSave").css('display', 'none');
                    $("#textWarningSave").html('');
                    $("#oldPassword").val('');
                    $("#newPassword").val('');
                    $("#newConfirmPassword").val('');
                });
                //close modal success
                $('#modalSuccess').on('hidden.bs.modal', function (e) {
                     window.location.href = '@Url.Action("Login", "Account")';
                });

                function btnChangePassword() {
                    var error = 0;
                    const msg = [];
                    var msgEx = "";
                    var alphanumeric = /^[a-zA-Z0-9]+$/;

                    var username = $("#username").val();
                    var oldPassword = $("#oldPassword").val();
                    var newPassword = $("#newPassword").val();
                    var newConfirmPassword = $("#newConfirmPassword").val();

                    if (oldPassword == "") {
                        ++error;
                        msg.push(error + ".Old password empty.");
                        $("#oldPassword").css({ "border": "1px solid #ff4d4d" });
                    }
                    if (newPassword == "") {
                        ++error;
                        msg.push(error + ".New password empty.");
                        $("#newPassword").css({ "border": "1px solid #ff4d4d" });

                    } else {
                        //check new pass
                        if (newPassword.length < 8) {
                            ++error;
                            msg.push(error + ".Your password must be at least 8 characters");
                        }
                        if (newPassword.length > 16) {
                            ++error;
                            msg.push(error + ".Your password must be at max 16 characters");
                        }
                        if (newPassword.search(/[a-z]/) < 0) {
                            ++error;
                            msg.push(error + ".Your password must contain at least one lower case letter.");
                        }
                        if (newPassword.search(/[A-Z]/) < 0) {
                            ++error;
                            msg.push(error + ".Your password must contain at least one upper case letter.");
                        }
                        if (newPassword.search(/[0-9]/) < 0) {
                            ++error;
                            msg.push(error + ".Your password must contain at least one digit.");
                        }
                        //if (newPassword.match(alphanumeric) === null) {
                        //    ++error;
                        //    msg.push(error + ".Your password must contain at least special char.");
                        //}
                    }

                    if (newConfirmPassword == "") {
                        ++error;
                        msg.push(error + ".Confirm password empty.");
                        $("#newConfirmPassword").css({ "border": "1px solid #ff4d4d" });
                    }

                    if (newPassword != "" && newConfirmPassword != "") {
                        if (newPassword != newConfirmPassword) {
                            ++error;
                            msg.push(error + ".New Password and Confirm Password not match.");
                        }
                    }
                    if (error > 0) {
                        $("#warningSave").css('display', 'block');
                        $("#textWarningSave").html(msg.join("<br>"));
                    } else {
                        $("#warningSave").css('display', 'none');
                        $.ajax({
                            type: 'POST',
                            cache: false,
                            url: '@Url.Action("ChangePassword", "Account")',
                            data: {
                                userName: username,
                                oldPassword: oldPassword,
                                newPassword: newPassword,
                            },
                            beforeSend: function () {
                                $("#iconLoading").html('<i class="fa fa-spinner fa-spin"></i>');
                                $("#btnChangePassword").prop('disabled', true);
                            },
                            success: function (result) {
                                msgEx = result.message;
                                if (result.status == "success") {
                                    $('#modalSuccess').modal({ backdrop: 'static', keyboard: false });
                                    setTimeout(function () {
                                        $('#modalChangePassword').modal('toggle');
                                    }, 500);
                                    @*$("#successSave").css('display', 'block');
                                    $("#textSuccessSave").html(result.message + '<br>'  + ' Automatic Logout Mobile order.' + '<br>' +'<b><u>Please logoff windows.</u></b>');
                                    $("#warningSave").css('display', 'none');
                                    setTimeout(function () {
                                        window.location.href = '@Url.Action("Login", "Account")';
                                    }, 5000);*@
                                } else {
                                    $("#successSave").css('display', 'none');

                                    $("#warningSave").css('display', 'block');
                                    if (msgEx.indexOf("an invocation") > -1) {
                                        msgEx = "New password has already been used.";
                                    }
                                    $("#textWarningSave").html(msgEx);
                                }
                            },
                            complete: function (res) {
                                $("#btnChangePassword").prop('disabled', false);
                                $("#iconLoading").html('');
                            },
                            error: function (ex) {
                                $("#btnChangePassword").prop('disabled', false);
                                $("#iconLoading").html('');
                                $("#successSave").css('display', 'none');

                                $("#warningSave").css('display', 'block');
                                $("#textWarningSave").html(ex);
                            }
                        }).fail(function (error) {
                            $("#successSave").css('display', 'none');

                            $("#warningSave").css('display', 'block');
                            $("#textWarningSave").html(error);
                        });
                    }
                }

            </script>

        </body>

    </html>